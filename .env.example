# ===========================================
# Idiotology KCI Agent - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ===== AI Models =====
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY_HERE
DEEPSEEK_API_KEY=YOUR_DEEPSEEK_API_KEY_HERE
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# ===== Public Data APIs =====
# 한국은행 ECOS (https://ecos.bok.or.kr/api/)
ECOS_API_KEY=your_ecos_key_here

# 공공데이터포털 (https://www.data.go.kr/)
DATA_GO_KR_KEY=your_data_go_kr_key_here

# ===== Database =====
DATABASE_PATH=./data/kci.db

# ===== Scraping Config =====
# 기준 점포 (강남구 역삼동)
BAEMIN_BBQ_STORE_ID=123456
BAEMIN_KYOCHON_STORE_ID=234567
BAEMIN_BHC_STORE_ID=345678

# ===== Notifications (Optional) =====
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# ===== Logging =====
LOG_LEVEL=INFO

# ===== KAMIS OpenAPI (aT 농수산물유통정보) =====
# 신청/키 발급: KAMIS > 고객센터 > Open-API
# cert_key, cert_id는 발급받은 값을 입력
KAMIS_CERT_KEY=your_kamis_cert_key_here
KAMIS_CERT_ID=your_kamis_cert_id_here

# (옵션) periodRetailProductList 기본 조회 코드
# - 품목/품종/등급 코드는 KAMIS '농축수산물 품목 및 등급 코드표'에서 확인
KAMIS_ITEMCATEGORYCODE=
KAMIS_ITEMCODE=
KAMIS_KINDCODE=
KAMIS_PRODUCTRANKCODE=04
KAMIS_COUNTRYCODE=
